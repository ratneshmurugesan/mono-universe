{
  "root": "apps/design-system-core-e2e",
  "sourceRoot": "apps/design-system-core-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/design-system-core-e2e/cypress.json",
        "devServerTarget": "design-system-core:storybook"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "design-system-core:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/design-system-core-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["design-system-core"]
}
